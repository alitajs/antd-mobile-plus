{
  "name": "ui",
  "scope": "alitajs",
  "version": 2,
  "builds": [{
    "src": "build.sh",
    "use": "@now/static-build",
    "config": {
      "distDir": "doc-dist"
    }
  }],
  "routes": [{
      "src": "/(.*[.js|.css])",
      "headers": {
        "Cache-Control": "max-age=63072000"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/404.html"
    }
  ],
  "github": {
    "enable": false
  },
  "alias": ["ui.alitajs.com"]
}